   "source": [
    "ops = ['+', '-', '*', '\\\\', '^', 'max', 'min']\n",
    "feats = ['x%d'%i for i in range(10)]\n",
    "consts = list(range(1, 21))\n",
    "nodes = ops + feats + consts"
   ]
